    "image_desktop": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "image_mobile": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
